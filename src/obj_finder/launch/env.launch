<launch>
  <param name="use_sim_time" value="true"/>

  <node pkg="tf2_ros" type="static_transform_publisher" name="base_to_base" args="0 0 0.0 0 0 0 base_footprint base_link" />
  <node pkg="tf2_ros" type="static_transform_publisher" name="base_to_laser" args="0 0 0.1 0 0 0 base_footprint base_scan" />
  <node pkg="tf2_ros" type="static_transform_publisher" name="base_to_camera" args="0.1 0.075 0.2 0 0 0 base_footprint camera_link" />
  <node pkg="tf2_ros" type="static_transform_publisher" name="camera_to_rgb" args="0 0 0 0 0 0 camera_link camera_rgb_frame" />
  <node pkg="tf2_ros" type="static_transform_publisher" name="rgb_to_optical" args="0 0 0 -1.5708 0 -1.5708 camera_rgb_frame camera_rgb_optical_frame" />

  <include file="$(find turtlebot3_gazebo)/launch/turtlebot3_house.launch" />
</launch>
